<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.shape.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.shape.Circle?>

<AnchorPane fx:id="root" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.edu.example.amongus.task.FixWiringController">
<children>
    <ImageView fx:id="background" fitHeight="720.0" fitWidth="1280.0" preserveRatio="true">
        <image>
            <Image url="@images/Fix_Wiring.png" />
        </image>
        </ImageView>

        <!-- 左边插口 -->
        <Circle fx:id="left1" layoutX="71.0" layoutY="190.0" radius="15.0" style="-fx-fill: red;" />
        <Circle fx:id="left2" layoutX="71.0" layoutY="383.0" radius="15.0" style="-fx-fill: blue;" />
        <Circle fx:id="left3" layoutX="71.0" layoutY="580.0" radius="15.0" style="-fx-fill: green;" />
        <Circle fx:id="left4" layoutX="71.0" layoutY="772.0" radius="15.0" style="-fx-fill: yellow;" />

        <!-- 右边插口 -->
        <Circle fx:id="right1" layoutX="832.0" layoutY="190.0" radius="15.0" style="-fx-fill: red;" />
        <Circle fx:id="right2" layoutX="825.0" layoutY="383.0" radius="15.0" style="-fx-fill: blue;" />
        <Circle fx:id="right3" layoutX="825.0" layoutY="580.0" radius="15.0" style="-fx-fill: green;" />
        <Circle fx:id="right4" layoutX="825.0" layoutY="772.0" radius="15.0" style="-fx-fill: yellow;" />
    </children>

</AnchorPane>
